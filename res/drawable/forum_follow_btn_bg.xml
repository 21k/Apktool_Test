<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <stroke android:width="0.5dip" android:color="@color/heise3_pressed" />
            <solid android:color="@color/dihuise1" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <stroke android:width="0.5dip" android:color="@color/heise3" />
            <solid android:color="@color/dihuise1" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <stroke android:width="0.5dip" android:color="@color/lanseqianse1_pressed" />
            <solid android:color="@color/dihuise8" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="2.0dip" />
            <stroke android:width="0.5dip" android:color="@color/lanseqianse1" />
            <solid android:color="@color/dihuise8" />
        </shape>
    </item>
</selector>