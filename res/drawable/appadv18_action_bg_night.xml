<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/appad_v18_button_pressed_stroke_night" />
            <solid android:color="@color/appad_v18_button_pressed_bg_night" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dip" android:color="@color/appad_v18_button_normal_stroke_night" />
            <solid android:color="@color/appad_v18_button_normal_bg_night" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
</selector>