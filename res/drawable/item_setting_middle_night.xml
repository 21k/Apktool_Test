<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/ssxinmian4_press_night" />
                </shape>
            </item>
            <item android:left="15.0dip" android:top="49.0dip">
                <shape android:shape="line">
                    <padding android:bottom="0.5dip" />
                    <stroke android:width="0.5dip" android:color="@color/ssxinxian1_press_night" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/ssxinmian4_night" />
                </shape>
            </item>
            <item android:left="15.0dip" android:top="49.0dip">
                <shape android:shape="line">
                    <padding android:bottom="0.5dip" />
                    <stroke android:width="0.5dip" android:color="@color/ssxinxian1_night" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>